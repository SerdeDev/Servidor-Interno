generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model operadoras {
  interlocutor BigInt
  nombre       String   @db.Char(120)
  estado       String   @db.Char(120)
  municipio    String   @db.Char(120)
  servicio     String   @db.Char(120)
  estatus      String?  @db.Char(50)
  estatusTec   Boolean?
  estatusComer Boolean?
  estatusRecau Boolean?

  @@ignore
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model catastro {
  id             Int      @default(autoincrement())
  interlocutor   BigInt
  fecha          DateTime
  tarifa         String?
  cuentaContrato BigInt
  ptoSuministro  BigInt
  monto          Decimal  @db.Decimal(18, 2)
  cnae           BigInt?
  instalacion    String?
  nombre         String
  direccion      String
  cicnor         String
  cedula         String?
  fechalta       String
  fechbaja       String
  contrato       BigInt
  telefono1      String?
  telefono2      String?

  @@ignore
}

model deudaTotal {
  interlocutor     BigInt
  fecha            DateTime
  interlocutorUser BigInt
  cuentaContrato   BigInt
  contrato         BigInt
  ptoSuministro    BigInt
  cnae             BigInt
  nombre           String
  aforo            Decimal  @db.Decimal(10, 2)
  total            Decimal  @db.Decimal(10, 2)
  parroquia        String?
  direccion        String?
  cantidadFacturas Int?
  cedula           String?
  telfonos         String?
  retail           String?
  centro           String?
  fechaBaja        String?
  id               Int      @id @default(autoincrement())

  @@index([cuentaContrato], map: "idx_deudatotal_cuentacontrato")
  @@index([interlocutor], map: "idx_interlocuto_deuda")
}

model recg {
  id               Int      @id @default(autoincrement())
  interlocutor     BigInt
  fecha            DateTime
  servicio         String?
  producto         String?
  cantidad         Int?
  numRegistro      Int?
  monto            Decimal  @db.Decimal(18, 2)
  iva              Decimal  @db.Decimal(18, 2)
  montoPlus        Decimal  @db.Decimal(18, 2)
  ivaPlus          Decimal  @db.Decimal(18, 2)
  retencionIva     Decimal  @db.Decimal(18, 2)
  retencionIvaPlus Decimal  @db.Decimal(18, 2)
  pagoAnticipo     Decimal  @db.Decimal(18, 2)
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model sigecom {
  estadoUser      String? @db.Char(120)
  municipioUser   String? @db.VarChar(300)
  parroquiaUser   String? @db.Char(120)
  direccionUser   String? @db.Char(300)
  documento       String? @db.Char(20)
  nombres         String? @db.Char(150)
  telefono1       String? @db.Char(120)
  telefono2       String? @db.Char(120)
  correo          String? @db.Char(120)
  fechaCreacion   String? @db.Char(120)
  estadoCC        String? @db.Char(120)
  municipioCC     String? @db.Char(120)
  parroquiaCC     String? @db.Char(120)
  cuenta          String? @db.Char(120)
  tarifa          String? @db.Char(120)
  device          String? @db.Char(120)
  cuentasap       String? @db.Char(120)
  zona            String? @db.Char(120)
  tipoCalle       String? @db.Char(120)
  nombreCalle     String? @db.Char(300)
  urbanizacion    String? @db.Char(300)
  puntoSuministro String? @db.Char(300)
  referencia      String? @db.Char(300)
  fechaCCuenta    String? @db.Char(120)
  estatusCC       String? @db.Char(120)

  @@index([correo], map: "idx_sigecom_correo")
  @@ignore
}

model acceso_bajas {
  acceso_bajas_id       Int               @id @unique(map: "acceso_bajas_id") @default(autoincrement()) @db.SmallInt
  acceso_bajas_name     String            @db.VarChar(255)
  acceso_bajas_surmane  String            @db.VarChar(255)
  acceso_bajas_password String            @db.VarChar(255)
  bajas                 bajas[]
  historialEnvios       historialEnvios[]
}

model bajas {
  bajas_id            Int          @id @unique(map: "bajas_id") @default(autoincrement()) @db.SmallInt
  bajas_contrato      String       @db.VarChar(255)
  bajas_correo_before String?      @db.VarChar(255)
  bajas_correo_after  String       @db.VarChar(255)
  acceso_bajas_id     Int          @db.SmallInt
  bajas_date_process  DateTime?    @default(now()) @db.Timestamp(0)
  acceso_bajas        acceso_bajas @relation(fields: [acceso_bajas_id], references: [acceso_bajas_id], onDelete: NoAction, map: "acceso_bajas_id_bajas_id")
}

model estr_pol_acceso {
  estr_pol_acceso_id      Int                   @unique(map: "unique_estr_pol_acceso_id") @default(autoincrement())
  estr_pol_acceso_descrip String?               @db.VarChar(255)
  estr_pol_acceso_sigla   String?               @db.VarChar(255)
  estr_pol_rel_acceso     estr_pol_rel_acceso[]
}

model estr_pol_edo {
  estr_pol_edo_id      Int                   @id(map: "unique_estr_pol_edo_id") @default(autoincrement())
  estr_pol_edo_descrip String                @db.VarChar
  estr_pol_mun         estr_pol_mun[]
  estr_pol_parr        estr_pol_parr[]
  estr_pol_rel_acceso  estr_pol_rel_acceso[]
}

model estr_pol_mun {
  estr_pol_mun_id      Int                   @id(map: "unique_estr_pol_mun_id") @default(autoincrement())
  estr_pol_mun_descrip String                @db.VarChar
  estr_pol_edo_id      Int
  estr_pol_edo         estr_pol_edo          @relation(fields: [estr_pol_edo_id], references: [estr_pol_edo_id], onDelete: NoAction, map: "fk_estr_pol_edo_id")
  estr_pol_parr        estr_pol_parr[]
  estr_pol_rel_acceso  estr_pol_rel_acceso[]
}

model estr_pol_parr {
  estr_pol_parr_id      Int                   @id(map: "unique_estr_pol_parr") @default(autoincrement())
  estr_pol_parr_descrip String                @db.VarChar(255)
  estr_pol_edo_id       Int
  estr_pol_mun_id       Int
  estr_pol_edo          estr_pol_edo          @relation(fields: [estr_pol_edo_id], references: [estr_pol_edo_id], onDelete: NoAction, map: "fk_estr_pol_edo_id")
  estr_pol_mun          estr_pol_mun          @relation(fields: [estr_pol_mun_id], references: [estr_pol_mun_id], onDelete: NoAction, map: "fk_estr_pol_mun_id")
  estr_pol_rel_acceso   estr_pol_rel_acceso[]
}

model estr_pol_rel_acceso {
  estr_pol_rel_acceso_id      BigInt          @id(map: "unique_estr_pol_rel_acceso_id") @default(autoincrement())
  estr_pol_edo_id             Int
  estr_pol_mun_id             Int
  estr_pol_parr_id            Int
  estr_pol_acceso_id          Int
  estr_pol_rel_acceso_descrip String          @db.VarChar(255)
  estr_pol_acceso             estr_pol_acceso @relation(fields: [estr_pol_acceso_id], references: [estr_pol_acceso_id], onDelete: NoAction, map: "fk_estr_pol_acceso_id")
  estr_pol_edo                estr_pol_edo    @relation(fields: [estr_pol_edo_id], references: [estr_pol_edo_id], onDelete: NoAction, map: "fk_estr_pol_edo_id")
  estr_pol_mun                estr_pol_mun    @relation(fields: [estr_pol_mun_id], references: [estr_pol_mun_id], onDelete: NoAction, map: "fk_estr_pol_mun_id")
  estr_pol_parr               estr_pol_parr   @relation(fields: [estr_pol_parr_id], references: [estr_pol_parr_id], onDelete: NoAction, map: "fk_estr_pol_parr_id")
}

model historialEnvios {
  historialEnvios_id        BigInt                   @id(map: "historialEnvios_id") @default(autoincrement())
  historialEnvios_startDate DateTime                 @default(now()) @db.Timestamp(6)
  historialEnvios_endDate   DateTime?                @db.Timestamp(6)
  historialEnvios_edo       String                   @db.VarChar(120)
  historialEnvios_mun       String                   @db.VarChar(120)
  acceso_bajas_id           Int
  acceso_bajas              acceso_bajas             @relation(fields: [acceso_bajas_id], references: [acceso_bajas_id], onDelete: NoAction, map: "fk_historialEnvios_acceso_bajas_id")
  historialEnviosDetalle    historialEnviosDetalle[]
}

model historialEnviosDetalle {
  historialEnviosDetalle_id BigInt          @id(map: "unique_historialEnviosDetalle_id") @default(autoincrement())
  historialEnvios_id        BigInt
  cuentacontrato            String          @db.VarChar(255)
  correo                    String          @db.VarChar(255)
  historialEnvios           historialEnvios @relation(fields: [historialEnvios_id], references: [historialEnvios_id], onDelete: NoAction, map: "fk_historialEnvios_id")
}

model operadoras_mod {
  nombre                String                  @db.Char(120)
  estado                String                  @db.Char(120)
  municipio             String                  @db.Char(120)
  servicio              String                  @db.Char(120)
  interlocutor          BigInt                  @id(map: "operadoras_copy1_pkey")
  ingreso_date          DateTime                @db.Date
  baja_date             DateTime                @db.Date
  recau_zconsulta       recau_zconsulta[]
  recau_zconsulta_copy1 recau_zconsulta_copy1[]
}

model employees_cargos {
  employees_cargos_id      Int                  @id(map: "cargos_pkey") @default(autoincrement())
  employees_gerencia_id    Int?
  employees_cargos_descrip String?
  employees_gerencias      employees_gerencias? @relation(fields: [employees_gerencia_id], references: [employees_gerencias_id], onDelete: NoAction, map: "fk_employees_gerencias_id")
  employees_user           employees_user[]
}

model employees_gerencias {
  employees_gerencias_id      Int                @id(map: "gerencias_pkey") @unique(map: "unique_employees_gerencias_id") @default(autoincrement())
  employees_gerencias_descrip String
  employees_cargos            employees_cargos[]
}

model employees_user {
  employees_user_id        Int               @id(map: "user_pkey") @unique(map: "unique_employees_user_id") @default(autoincrement())
  employees_user_name      String
  employees_user_surname   String?
  employees_user_birthdate DateTime          @db.Date
  employees_user_extension Int?
  employees_user_correo    String?
  employees_cargo_id       Int?
  employees_user_password  String?           @db.VarChar(20)
  employees_cargos         employees_cargos? @relation(fields: [employees_cargo_id], references: [employees_cargos_id], onDelete: Cascade, map: "usuario_cargo")
}

model recau_zconsulta {
  recau_zconsulta_id                 BigInt         @id(map: "unique_recau_zconsulta_id") @default(autoincrement())
  recau_zconsulta_fecha              DateTime       @db.Date
  interlocutor                       BigInt
  recau_zconsulta_pagoanticip        Decimal        @default(0) @db.Decimal(10, 2)
  recau_zconsulta_aseo               Decimal        @default(0) @db.Decimal(10, 2)
  recau_zconsulta_aseoplus           Decimal        @default(0) @db.Decimal(10, 2)
  recau_zconsulta_iva_aseoplus       Decimal        @default(0) @db.Decimal(10, 2)
  recau_zconsulta_iva_aseo           Decimal        @default(0) @db.Decimal(10, 2)
  recau_zconsulta_medioambiental     Decimal        @default(0) @db.Decimal(10, 2)
  recau_zconsulta_iva_medioambiental Decimal        @default(0) @db.Decimal(10, 2)
  recau_zconsulta_serv_esp           Decimal        @default(0) @db.Decimal(10, 2)
  recau_zconsulta_iva_serv_esp       Decimal        @default(0) @db.Decimal(10, 2)
  recau_zconsulta_serv_amb           Decimal        @default(0) @db.Decimal(10, 2)
  recau_zconsulta_iva_serv_amb       Decimal        @default(0) @db.Decimal(10, 2)
  recau_zconsulta_pagoanticip_2      Decimal        @default(0) @db.Decimal(10, 2)
  recau_zconsulta_relleno            Decimal        @default(0) @db.Decimal(10, 2)
  recau_zconsulta_iva_relleno        Decimal        @default(0) @db.Decimal(10, 2)
  recau_zconsulta_total              Decimal        @default(0) @db.Decimal(10, 2)
  recau_zconsulta_vacio              Decimal        @db.Decimal(10, 2)
  operadoras_mod                     operadoras_mod @relation(fields: [interlocutor], references: [interlocutor], onDelete: NoAction, map: "fk_interlocutor")
}

model recau_zconsulta_copy1 {
  recau_zconsulta_id                 BigInt         @id @default(autoincrement())
  recau_zconsulta_fecha              DateTime       @db.Date
  interlocutor                       BigInt
  recau_zconsulta_pagoanticip        Decimal        @default(0) @db.Decimal(10, 2)
  recau_zconsulta_aseo               Decimal        @default(0) @db.Decimal(10, 2)
  recau_zconsulta_aseoplus           Decimal        @default(0) @db.Decimal(10, 2)
  recau_zconsulta_iva_aseoplus       Decimal        @default(0) @db.Decimal(10, 2)
  recau_zconsulta_iva_aseo           Decimal        @default(0) @db.Decimal(10, 2)
  recau_zconsulta_medioambiental     Decimal        @default(0) @db.Decimal(10, 2)
  recau_zconsulta_iva_medioambiental Decimal        @default(0) @db.Decimal(10, 2)
  recau_zconsulta_serv_esp           Decimal        @default(0) @db.Decimal(10, 2)
  recau_zconsulta_iva_serv_esp       Decimal        @default(0) @db.Decimal(10, 2)
  recau_zconsulta_serv_amb           Decimal        @default(0) @db.Decimal(10, 2)
  recau_zconsulta_iva_serv_amb       Decimal        @default(0) @db.Decimal(10, 2)
  recau_zconsulta_pagoanticip_2      Decimal        @default(0) @db.Decimal(10, 2)
  recau_zconsulta_relleno            Decimal        @default(0) @db.Decimal(10, 2)
  recau_zconsulta_iva_relleno        Decimal        @default(0) @db.Decimal(10, 2)
  recau_zconsulta_total              Decimal        @default(0) @db.Decimal(10, 2)
  recau_zconsulta_vacio              Decimal        @db.Decimal(10, 2)
  operadoras_mod                     operadoras_mod @relation(fields: [interlocutor], references: [interlocutor], onDelete: NoAction)
}
